type UserId = nat64;
type CredentialId = vec nat8;
type Alias = text;

service : {
  register : (UserId, Alias, principal, opt CredentialId) -> ();
  add : (UserId, Alias; principal, opt CredentialId) -> ();
  remove : (UserId, principal) -> ();
  lookup : (UserId) -> (vec (Alias, principal, opt CredentialId));
}
